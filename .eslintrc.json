{
    "extends": [
		"react-app"
	],
    "rules": {
        "no-trailing-spaces": "warn",
        "no-multiple-empty-lines": ["warn", {"max": 1}],
        "eol-last": ["warn"],
        "@typescript-eslint/no-unused-vars": [
            "warn",
            {
                "varsIgnorePattern": "_"
            }
        ]
    }
}
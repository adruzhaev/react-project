{
  "name": "react-project",
  "version": "1.0.0",
  "description": "Building react project from scratch",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --env MODE=development --open --hot",
    "build": "webpack --env MODE=production",
    "test": "tsc && eslint . --ext .js,.jsx,.ts,.tsx --ignore-pattern dist/",

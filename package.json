  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adruzhaev/react-project.git"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "author": "Andrey Druzhaev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adruzhaev/react-project/issues"
  },
  "homepage": "https://github.com/adruzhaev/react-project#readme",
  "dependencies": {
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "babel-loader": "^8.2.5",
    "eslint": "^8.26.0",
    "eslint-config-react-app": "^7.0.1",
    "html-webpack-plugin": "^5.5.0",
